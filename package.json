{
  "name": "paas-portal",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "local": "vue-cli-service serve --mode local",
    "start": "npm run api-gen-local && vue-cli-service serve --mode local",
    "dev": "npm run api-gen-dev && vue-cli-service serve --mode dev",
    "sso": "npm run api-gen-sso && vue-cli-service serve --mode sso",
    "k8s": "npm run api-gen-k8s && vue-cli-service serve --mode k8s",
    "nds": "npm run api-gen-nds && vue-cli-service serve --mode nds",
    "build-dev": "npm run api-gen-dev && vue-cli-service build --mode dev",
    "build-sso": "cross-env NODE_ENV=sso && npm run api-gen-sso && vue-cli-service build --mode sso",
    "build-k8s": "cross-env NODE_ENV=k8s && npm run api-gen-k8s && vue-cli-service build --mode k8s",
    "build-nds": "cross-env NODE_ENV=nds && npm run api-gen-nds && vue-cli-service build --mode nds",
    "build-stg": "vue-cli-service build --mode stg",
    "build-prod": "cross-env NODE_ENV=prod && npm run api-gen-prod && vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "api-gen-local": "cross-env API_GEN_ENV=local node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-dev": "cross-env API_GEN_ENV=dev node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-stg": "cross-env API_GEN_ENV=stg node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-prod": "cross-env API_GEN_ENV=prod node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-sso": "cross-env API_GEN_ENV=sso node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-k8s": "cross-env API_GEN_ENV=k8s node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js",
    "api-gen-nds": "cross-env API_GEN_ENV=nds node .api-gen/api-gen.js && eslint --fix ./src/lib/request.js"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "apexcharts": "^3.27.3",
    "axios": "^0.24.0",
    "chart.js": "^4.3.2",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "json2yaml": "^1.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "qs": "^6.11.1",
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.6.2",
    "vue-chartjs": "^5.2.0",
    "vue-cli-plugin-yaml": "^1.0.2",
    "vue-codemirror": "^4.0.6",
    "vue-loader": "^17.2.2",
    "vue-router": "^3.2.0",
    "vue-uuid": "^3.0.0",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^4.1.0",
    "xterm": "^4.17.0",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "dotenv": "^16.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.2.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "swagger-js-codegen": "^1.13.0",
    "vue-cli-plugin-vuetify": "~2.4.5",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "git add"
    ]
  }
}
